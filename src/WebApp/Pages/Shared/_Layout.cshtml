<!DOCTYPE html>
<html lang="en">
<head>
    <title>prometheus web exporter</title>
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <meta charset="utf-8" />
    <link rel="stylesheet" asp-append-version="true" href="~/css/app.css" />
    <link rel="shortcut icon" asp-append-version="true" href="~/media/favicon.png" />
    @await RenderSectionAsync("styles", false)
</head>
<body>

<nav class="navbar">
    <div class="logo"></div>
    <a asp-page="/Index">web - exporter</a>
</nav>

<div class="container">
    @RenderBody()
</div>

<script asp-append-version="true" src="~/js/app.js"></script>
@await RenderSectionAsync("scripts", false)
@Html.ReactInitJavaScript()

</body>
</html>
